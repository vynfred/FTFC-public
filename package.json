{
  "name": "ftfc",
  "version": "1.0.0",
  "description": "FTFC Website",
  "main": "index.js",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:high-memory": "node build-high-memory.js",
    "build:icons": "node scripts/convert-svg-to-png.js",
    "build:deploy": "node scripts/build-and-deploy.js",
    "build:analyze": "ANALYZE=true npm run build:high-memory",
    "build:small": "DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false npm run build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "emulators": "firebase emulators:start",
    "analyze": "node scripts/analyze-bundle.js",
    "lint": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx,css}",
    "deploy:hosting": "firebase deploy --only hosting --project ftfc-start",
    "deploy:functions": "firebase deploy --only functions --project ftfc-start",
    "deploy:small": "npm run build:small && npm run deploy:hosting",
    "build:deploy:small": "node scripts/build-small-and-deploy.js",
    "install:optimization": "node scripts/install-optimization-deps.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:deps": "node scripts/optimize-dependencies.js",
    "analyze:costs": "node scripts/estimate-firebase-costs.js",
    "analyze:webpack": "node scripts/optimize-webpack.js",
    "analyze:large-deps": "node scripts/optimize-large-deps.js",
    "analyze:firebase": "node scripts/optimize-firebase-hosting.js",
    "optimize": "npm run analyze:deps && npm run analyze:webpack && npm run analyze:costs && npm run analyze:large-deps && npm run analyze:firebase",
    "deploy:optimized": "node scripts/optimize-and-deploy.js"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.2.2",
    "@sentry/react": "^9.2.0",
    "@stripe/stripe-js": "^5.7.0",
    "@tinymce/tinymce-react": "^4.0.0",
    "assemblyai": "^4.9.0",
    "assert": "^2.1.0",
    "axios": "^1.7.9",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.8",
    "crypto-browserify": "^3.12.1",
    "date-fns": "^2.0.0",
    "dotenv": "^16.0.3",
    "events": "^3.3.0",
    "file-saver": "^2.0.5",
    "firebase": "^9.0.0",
    "googleapis": "^144.0.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "papaparse": "^5.4.1",
    "path-browserify": "^1.0.1",
    "plaid": "^31.1.0",
    "process": "^0.11.10",
    "prop-types": "^15.8.0",
    "querystring-es3": "^0.2.1",
    "react": "^18.0.0",
    "react-app-rewired": "^2.2.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.0.0",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^5.0.1",
    "react-tabs": "^6.1.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "stripe": "^17.7.0",
    "url": "^0.11.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "firebase-tools": "^12.0.0",
    "husky": "^7.0.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "prettier": "^2.0.0",
    "sharp": "^0.34.1",
    "source-map-explorer": "^2.5.3",
    "string-replace-loader": "^3.1.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
